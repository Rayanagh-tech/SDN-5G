{
    "version": "8.11.0",
    "objects": [
        {
            "id": "sdn-index-pattern",
            "type": "index-pattern",
            "attributes": {
                "title": "sdn-*",
                "timeFieldName": "@timestamp",
                "fields": "[]"
            }
        },
        {
            "id": "sdn-bandwidth-visualization",
            "type": "visualization",
            "attributes": {
                "title": "Bandwidth per Slice",
                "visState": {
                    "type": "line",
                    "params": {
                        "type": "line",
                        "grid": {"categoryLines": false},
                        "categoryAxes": [{"id": "CategoryAxis-1", "type": "category", "position": "bottom"}],
                        "valueAxes": [{"id": "ValueAxis-1", "type": "value", "position": "left", "title": {"text": "Bandwidth (Mbps)"}}],
                        "seriesParams": [{"type": "line", "mode": "normal", "show": true}],
                        "legendPosition": "right"
                    },
                    "aggs": [
                        {"id": "1", "type": "avg", "params": {"field": "bandwidth_mbps"}, "schema": "metric"},
                        {"id": "2", "type": "date_histogram", "params": {"field": "@timestamp", "interval": "auto"}, "schema": "segment"},
                        {"id": "3", "type": "terms", "params": {"field": "slice_name", "size": 5}, "schema": "group"}
                    ]
                },
                "description": "Shows bandwidth over time for each slice"
            }
        },
        {
            "id": "sdn-latency-visualization",
            "type": "visualization",
            "attributes": {
                "title": "Latency Comparison",
                "visState": {
                    "type": "histogram",
                    "params": {
                        "type": "histogram",
                        "grid": {"categoryLines": false},
                        "categoryAxes": [{"id": "CategoryAxis-1", "type": "category", "position": "bottom"}],
                        "valueAxes": [{"id": "ValueAxis-1", "type": "value", "position": "left", "title": {"text": "Latency (ms)"}}]
                    },
                    "aggs": [
                        {"id": "1", "type": "avg", "params": {"field": "latency_ms"}, "schema": "metric"},
                        {"id": "2", "type": "terms", "params": {"field": "slice_name", "size": 5}, "schema": "segment"}
                    ]
                },
                "description": "Compares average latency across slices"
            }
        },
        {
            "id": "sdn-jitter-visualization",
            "type": "visualization",
            "attributes": {
                "title": "Jitter per Slice",
                "visState": {
                    "type": "histogram",
                    "params": {
                        "type": "histogram"
                    },
                    "aggs": [
                        {"id": "1", "type": "avg", "params": {"field": "jitter_ms"}, "schema": "metric"},
                        {"id": "2", "type": "terms", "params": {"field": "slice_name", "size": 5}, "schema": "segment"}
                    ]
                },
                "description": "Shows jitter distribution per slice"
            }
        },
        {
            "id": "sdn-sla-violations",
            "type": "visualization",
            "attributes": {
                "title": "SLA Violations Count",
                "visState": {
                    "type": "metric",
                    "params": {
                        "type": "metric",
                        "fontSize": 60
                    },
                    "aggs": [
                        {"id": "1", "type": "count", "params": {}, "schema": "metric"}
                    ]
                },
                "kibanaSavedObjectMeta": {
                    "searchSourceJSON": {
                        "query": {"match": {"sla_status": "violated"}},
                        "filter": []
                    }
                },
                "description": "Counts total SLA violations"
            }
        },
        {
            "id": "sdn-packet-loss",
            "type": "visualization",
            "attributes": {
                "title": "Packet Loss Rate",
                "visState": {
                    "type": "gauge",
                    "params": {
                        "type": "gauge",
                        "addLegend": true,
                        "gauge": {
                            "minValue": 0,
                            "maxValue": 100,
                            "scale": {"show": true}
                        }
                    },
                    "aggs": [
                        {"id": "1", "type": "avg", "params": {"field": "packet_loss_pct"}, "schema": "metric"},
                        {"id": "2", "type": "terms", "params": {"field": "slice_name", "size": 5}, "schema": "group"}
                    ]
                },
                "description": "Gauge showing packet loss percentage"
            }
        },
        {
